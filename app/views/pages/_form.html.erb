
<% semantic_form_for @page do |form| %>
  <%= form.input :parent, :label_method => :title %>
  <%= form.input :title %>
  
  <div id="page_advanced" class="optional">
    <%= form.inputs :file_name, :permalink, :visible, :active, :meta_keywords, :meta_description %>
  </div>
  <%= form.buttons %>
<% end %>


